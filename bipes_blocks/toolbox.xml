<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
  <category name="Python">
    <block type="exec_python"></block>
  </category>
  <category name="Temporização">
    <block type="delay_seconds">
      <value name="TIME">
        <block type="math_number">
          <field name="NUM">1</field>
        </block>
      </value>
    </block>
    <block type="delay_microseconds">
      <value name="TIME">
        <block type="math_number">
          <field name="NUM">1000</field>
        </block>
      </value>
    </block>
  </category>
  <category name="Machine">
    <block type="reset"></block>
    <block type="gpio_set"></block>
  </category>
</xml>